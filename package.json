{
  "name": "taledeck",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently --kill-others 'npm run dev:vite'",
    "dev:vite": "vite --host",
    "prebuild": "npm run scripts:install && npm run scripts:use",
    "build": "run-p type-check build-only",
    "build-only": "vite build",
    "preview": "vite preview",
    "test:unit": "vitest --environment jsdom --root src/",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "scripts:install": "npm --prefix ./scripts install",
    "scripts:use": "npm --prefix ./scripts run use",
    "watch": "echo 'nothing to watch'",
    "prepare": "husky install",
    "test": "npm run lint && npm run type-check"
  },
  "dependencies": {
    "@directus/sdk": "^10.3.3",
    "@nirazul/scss-mq": "^1.2.1",
    "@nirazul/scss-utils": "^1.2.0",
    "@vueuse/core": "^10.2.0",
    "arg": "^5.0.2",
    "gl-matrix": "^3.4.3",
    "lodash-es": "^4.17.21",
    "pinia": "^2.0.30",
    "screenfull": "^6.0.2",
    "string-args-parser": "^1.0.4",
    "superjson": "^1.12.2",
    "three": "^0.155.0",
    "three-mesh-ui": "^6.5.4",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6",
    "yarn-bound": "^0.4.3"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^0.11.0",
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/jsdom": "^21.1.1",
    "@types/lodash-es": "^4.17.6",
    "@types/minimist": "^1.2.2",
    "@types/node": "^20.3.1",
    "@types/three": "^0.155.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.2.6",
    "@vue/tsconfig": "^0.4.0",
    "concurrently": "^8.2.0",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "husky": "^8.0.3",
    "jsdom": "^24.0.0",
    "npm-run-all": "^4.1.5",
    "path-browserify": "^1.0.1",
    "prettier": "^2.7.1",
    "sass": "^1.57.1",
    "typescript": "^5.1.3",
    "vite": "^4.0.0",
    "vite-plugin-mkcert": "^1.15.0",
    "vite-svg-loader": "^5.1.0",
    "vitest": "^1.2.2",
    "vue-i18n": "^9.3.0-beta.19",
    "vue-tsc": "^1.0.12"
  }
}
